{
    "pep723": {
        "fileMatch": ["pep723"]
    },
    "pip-compile": {
        "fileMatch": ["pip-compile.txt"],
        "lockFileMaintenance": {
            "enabled": true,
            "branchTopic": "pip-compile-refresh",
            "commitMessageAction": "Refresh pip-compile outputs"
        }
    },
    "pip_requirements": {
        "fileMatch": [
            "(^|/)[\\w-]*requirements([-.]\\w+)?\\.(txt|pip)$"
        ]
    },
    "pip_setup": {
        "fileMatch": [
            "pip-setup\\.py$"
        ]
    },
    "poetry": {
        "fileMatch": [
            "(^|/)pyproject_poetry\\.toml$"
        ]
    }
}